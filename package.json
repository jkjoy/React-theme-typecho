{
  "name": "dynamic-blog",
  "version": "1.0.0",
  "description": "A dynamic blog application that fetches content from an API",
  "main": "index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "server": "node api/server.js",
    "proxy": "node proxy-server.js",
    "dev": "concurrently \"npm run server\" \"npm run start\"",
    "dev:proxy": "concurrently \"npm run proxy\" \"npm run start\"",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "axios": "^1.3.5",
    "body-parser": "^1.20.2",
    "dayjs": "^1.11.7",
    "marked": "^4.3.0",
    "md5": "^2.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.8.0",
    "react-router-dom": "^6.10.0",
    "react-scripts": "5.0.1",
    "react-syntax-highlighter": "^15.5.0"
  },
  "devDependencies": {
    "concurrently": "^8.0.1",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "http-proxy-middleware": "^3.0.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
